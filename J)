using System;

namespace Matheus atv-1_
{
	class Program
	{
		public static void Main(string[] args)
		{
		
			
		            Console.Write("Digite o número da sua casa:");
		             string NumCasa = Convert.ToString(Console.ReadLine());
		             
		              Console.Write("Digite o nome da sua rua:");
		              string rua = Convert.ToString(Console.ReadLine());
		              
		               Console.Write("Digite o nome do seu bairro:");
		               string  bairro = Convert.ToString(Console.ReadLine());
		               
		               Console.Write("Digite o nome da sua cidade:");
		               string cidade = Convert.ToString(Console.ReadLine());
		               
		               
		               Console.Write("Digite o nome do seu estado:");
		               	string estado = Convert.ToString(Console.ReadLine());
		           
	         
			Console.WriteLine(" seu endereço:" + NumCasa + rua + bairro + cidade + estado);
				
